<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="600.0" style="-fx-background-color: #f7ce34;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mimic.mimic.GameOverController">
   <children>
      <VBox alignment="CENTER" layoutY="261.0" prefHeight="454.0" prefWidth="600.0" spacing="20.0">
         <children>
            <Circle fx:id="circImgInicio" fill="DODGERBLUE" radius="60.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
            <StackPane prefHeight="32.0" prefWidth="560.0">
               <children>
                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="37.0" stroke="BLACK" strokeType="INSIDE" width="200.0" StackPane.alignment="BOTTOM_CENTER" />
                  <Label fx:id="nickname" text="Nickname" />
               </children>
            </StackPane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label style="-fx-text-fill: #ffffff;" text="Game Over">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <Label fx:id="score" style="-fx-text-fill: #ffffff;" text="Puntuacion: ">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
            </HBox>
            <Button fx:id="bn1" mnemonicParsing="false" onAction="#Osu" prefHeight="35.0" prefWidth="302.0" style="-fx-background-color: #424242; -fx-text-fill: #ffffff;" text="PRACTICE AGAIN" />
            <Button fx:id="bn3" mnemonicParsing="false" onAction="#Puntuation" prefHeight="35.0" prefWidth="302.0" style="-fx-background-color: #424242; -fx-text-fill: #ffffff;" text="PUNTUACION" />
            <Button fx:id="bn2" mnemonicParsing="false" onAction="#Atras" prefHeight="35.0" prefWidth="302.0" style="-fx-background-color: #424242; -fx-text-fill: #ffffff;" text="ATRAS" />
            <Label text="Mimic Games" textFill="#1130a1">
               <font>
                  <Font size="21.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER" layoutY="8.0" prefHeight="285.0" prefWidth="592.0">
         <children>
            <ImageView fitHeight="214.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/banner%20.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <Label layoutX="253.0" layoutY="244.0" text="Version Demo 0.1" />
   </children>
</AnchorPane>
